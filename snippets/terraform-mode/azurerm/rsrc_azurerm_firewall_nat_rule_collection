# -*- mode: snippet -*-
# name: resource azurerm_firewall_nat_rule_collection
# key: key
# --
resource "azurerm_firewall_nat_rule_collection" "$1" {
  action = $2
  azure_firewall_name = $3
  name = $4
  priority = $5
  resource_group_name = $6

  rule {
    destination_addresses = $7
    destination_ports = $8
    name = $9
    protocols = $10
    source_addresses = $11
    translated_address = $12
    translated_port = $13
  }
}$0
